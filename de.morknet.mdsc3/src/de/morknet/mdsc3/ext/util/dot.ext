import mdsc3;

extension de::morknet::mdsc3::ext::util::constants;

String getDotColor(Machine m) :
	"grey";

String getDotColor(Host m) :
	"green";

String getDotColor(Printer m) :
	"blue";

String getDotColor(Switch m) :
	"yellow";

String getDotFile(Model m) :
	getInfrastructure(m) + ".dot";

String getMapFile(Model m) :
	getInfrastructure(m) + ".gif";
